<table>
    <tr>
        <td>ID</td>
        <td>Company</td>
        <td>Name</td>
    </tr>
    <% for(var i=0; i < noticias.length; i++) { %>
        <tr>
            <td>
                <%= noticias[i].id %>
            </td>
            <td>
                <%= noticias[i].titulo %>
            </td>
            <td>
                <%= noticias[i].descricao %>
            </td>
            <td>
                <%= noticias[i].resumo %>
            </td>
            <td>
                <%= noticias[i].autor %>
            </td>
            <td>
                <%= noticias[i].data_noticia %>
            </td>
        </tr>
        <% } %>
</table>